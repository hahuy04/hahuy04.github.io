#include<stdio.h>
#include<conio.h>
int Fibo(int n)
{ 
	if(n==0)
	{
		return 0;
	}
	if(n==1)
	{
		return 1;
	}
	else
	{
		return Fibo(n-1)+Fibo(n-2);
	}
}
int main()
{
	int n,S;
	printf("//Nhap vao so nguyen n: ");
	scanf("%d",&n);
	S=Fibo(n-1)+Fibo(n-2);
	printf("//So hang thu %d cua day so Fibonaci la: %d",n,S);
}
