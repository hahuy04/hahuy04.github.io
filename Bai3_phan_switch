#include<stdio.h>
#include<conio.h>
 int main()
{
	int n, can, chi;
	printf("nhap vao n: ");scanf("%d",&n);
	can=n%10;
	chi=n%12;
	switch(can)
{
	case 0:
		printf("canh");break;
	case 1:
		printf("tan");break;
	case 2:
		printf("nham");break;
	case 3:
		printf("quy");break;
	case4:
		printf("giap");break;
	case 5:
		printf("at");break;
	case 6:
		printf("binh");break;
	case 7:
		printf("dinh");break;
	case 8:
		printf("mau");break;
	case 9:
		printf("ky");break;
}
	switch(chi)
{
	case 0:
		printf("than");break;
	case 1:
		printf("dau");break;
	case 2:
		printf("tuat");break;
	case 3:
		printf("hoi");break;
	case 4:
		printf("ty");break;
	case 5:
		printf("suu");break;
	case 6:
		printf("dan");break;
	case 7:
		printf("mao");break;
	case 8:
		printf("thin");break;
	case 9:
		printf("ty");break;
	case 10:
	printf("ngo");break;
	case 11:
	printf("mui");break;
}
 getch();
}
